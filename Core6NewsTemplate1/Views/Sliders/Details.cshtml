@model WebOS.Models.Slider

@{
    ViewData["Title"] = "معلومات بانر";
}

<div class="slider-section-03"></div>
<div class="px-5">

    <h4>@Model.Title</h4>
    <hr />
    <dl class="row">
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Title)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnTitle)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SubTitle)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnSubTitle)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Image)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.indx)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AddDate)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ContentAlignment)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Url)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UrlText)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnUrlText)
        </dd>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Status)
        </dd>
    </dl>
</div>
